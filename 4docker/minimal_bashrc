export PS1="$([ $(id -u)=0 ] &&echo -e "\e[31m"||echo -e "\e[32m")\u@\e[33m\h:\w# \e[0m"
alias ..='cd ..'
alias ll='ls --color=auto -Xalh --group-directories-first'
alias ls='ls --color=auto -h --group-directories-first'
alias nano='nano -c'
alias nstt='netstat -plunat'
alias printenv='LC_ALL=C; printenv | sort'
